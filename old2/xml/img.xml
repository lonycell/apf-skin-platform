<a:skin xmlns:a="http://ajax.org/2005/aml">
    <a:img name="imgbasic">
        <a:style><![CDATA[
            .imgbasic {
                height  : 32px;
                padding : 0px 0 15px 16px;
            }
        ]]></a:style>
    
        <a:presentation>
            <a:main image="img/@src">
                <div class="imgbasic">
                    <img src="" />
                </div>
            </a:main>
        </a:presentation>
    </a:img>
    
    <a:img name="img">
        <a:style><![CDATA[
            .img {
                padding    : 10px;
                border     : 1px solid #c3c3c3;
                background : white no-repeat 50% 50%;
                overflow   : hidden;
                position   : relative;
                text-align : center;
            }
            
            .img img {
                margin   : auto;
                position : relative;
            }
        ]]></a:style>
    
        <a:presentation>
            <a:main image="img/@src">
                <div class="img">
                    <img src="" />
                </div>
            </a:main>
        </a:presentation>
    </a:img>
</a:skin>