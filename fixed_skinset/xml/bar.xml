<a:bar name="bar">
    <a:style><![CDATA[
        .bar {
            color       : #4b4b4b;
            font-family : Tahoma;
            font-size   : 8pt;
            padding     : 20px 0 26px 0;
            _padding    : 20px 0 26px 0;
            overflow    : hidden;
        }
    
        .bar .contents {
            clear        : both;
            border-top   : 1px solid #c3c3c3;
            border-left  : 1px solid #c3c3c3;
            border-right : 1px solid #c3c3c3;
            background   : #fafbfc;
            padding      : 7px;
            height       : 100%;
            padding      : 15px;
            margin-top   : -20px;
            position     : relative;
        }
        .barNoRightRb .contents {
            border-right : none;
        }
    
        .barNoRightLbGray .contents {
            background-color : #f1f1f1;
        }
    
        .bar .barFooter {
            clear      : both;
            height     : 15px;
            background : url(images/tab_pag_lb.png) no-repeat bottom left;
            position   : absolute;
            bottom     : 0;
            width      : 100%;
        }
    
        .barNoRightLbGray .barFooter {
            background : url(images/tab_pag_lb_special.png) no-repeat bottom left;
        }
    
        .bar .barFooter div{
            background : url(images/tab_pag_rb.png) no-repeat bottom right;
        }
        .barNoRightRb .barFooter div {
            background : url(images/tab_pag_rb_special.png) no-repeat bottom right;
        }
        .barNoRightRbBorder .barFooter div {
            background : url(images/tab_pag_rb_specia_border.png) no-repeat bottom right;
        }
        .barNoRightLbGray .barFooter div {
            background : url(images/tab_pag_rb_special_gray.png) no-repeat bottom right;
        }
        
        .bar .barFooter div div {
            border-bottom    : 1px solid #c3c3c3;
            background-color : #fafbfc;
            background-image : none;
            height           : 14px;
            margin           : 0 14px;
        }
    
        .barNoRightLbGray .barFooter div div {
            background-color : #f1f1f1;
        }
    ]]></a:style>
    <a:style condition="apf.isIE6">
        .bar {
            padding : 20px 0 0 0;
        }
        
        .bar .contents {
            padding-bottom : 0;
        }
    </a:style>
    <a:presentation>
        <a:main container="div">
            <div class="bar">
                <div class="contents"> </div>
                <div class="barFooter">
                    <div>
                        <div> </div>
                    </div>
                </div>
            </div>
        </a:main>
    </a:presentation>
</a:bar>

<a:bar name="basic">
    <a:style><![CDATA[
        .basic {
            position : relative;
            overflow : hidden;
        }
    ]]></a:style>
    
    <a:presentation>
        <a:main container=".">
            <div class="basic">
            </div>
        </a:main>
    </a:presentation>
</a:bar>

<a:bar name="packager">
    <a:style><![CDATA[
        .packager .loading {
            width    : 800px;
            height   : 23px;
