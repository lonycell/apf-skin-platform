<a:bar name="bar">
    <a:style><![CDATA[
        .bar {
            color       : #4b4b4b;
            font-family : Tahoma;
            font-size   : 8pt;
            padding     : 20px 0 26px 0;
            _padding    : 20px 0 26px 0;
            overflow    : hidden;
        }
    
        .bar .contents {
            clear        : both;
            border-top   : 1px solid #c3c3c3;
            border-left  : 1px solid #c3c3c3;
            border-right : 1px solid #c3c3c3;
            background   : #fafbfc;
            padding      : 7px;
            height       : 100%;
            padding      : 15px;
            margin-top   : -20px;
            position     : relative;
        }
        .barNoRightRb .contents {
            border-right : none;
        }
    
        .barNoRightLbGray .contents {
            background-color : #f1f1f1;
        }
    
        .bar .barFooter {
            clear      : both;
            height     : 15px;
            background : url(images/tab_pag_lb.png) no-repeat bottom left;
            position   : absolute;
            bottom     : 0;
            width      : 100%;
        }
    
        .barNoRightLbGray .barFooter {
            background : url(images/tab_pag_lb_special.png) no-repeat bottom left;
        }
    
        .bar .barFooter div{
            background : url(images/tab_pag_rb.png) no-repeat bottom right;
        }
        .barNoRightRb .barFooter div {
            background : url(images/tab_pag_rb_special.png) no-repeat bottom right;
        }
        .barNoRightRbBorder .barFooter div {
            background : url(images/tab_pag_rb_specia_border.png) no-repeat bottom right;
        }
        .barNoRightLbGray .barFooter div {
            background : url(images/tab_pag_rb_special_gray.png) no-repeat bottom right;
        }
        
        .bar .barFooter div div {
            border-bottom    : 1px solid #c3c3c3;
            background-color : #fafbfc;
            background-image : none;
            height           : 14px;
            margin           : 0 14px;
        }
    
        .barNoRightLbGray .barFooter div div {
            background-color : #f1f1f1;
        }
    ]]></a:style>
    <a:style condition="apf.isIE6">
        .bar {
            padding : 20px 0 0 0;
        }
        
        .bar .contents {
            padding-bottom : 0;
        }
    </a:style>
    <a:presentation>
        <a:main container="div">
            <div class="bar">
                <div class="contents"> </div>
                <div class="barFooter">
                    <div>
                        <div> </div>
                    </div>
                </div>
            </div>
        </a:main>
    </a:presentation>
</a:bar>

<a:bar name="basic">
    <a:style><![CDATA[
        .basic {
            position : relative;
            overflow : hidden;
        }
    ]]></a:style>
    
    <a:presentation>
        <a:main container=".">
            <div class="basic">
            </div>
        </a:main>
    </a:presentation>
</a:bar>

<a:bar name="bardown">
    <a:style><![CDATA[
        .bardown {
            font-family : Tahoma;
            font-size   : 11px;
            padding     : 0 0 10px 0;
            text-align  : left;
            z-index     : 1;
            position    : relative;
        }
    
        .bardown .content {
            border-left : 1px solid #c3c3c3;
            padding     : 3px 5px 0 5px;
            height      : 100%;
            position    : relative;
            background  : white;
            overflow    : visible;
            margin      : 0 8px 0 0;
        }
        
        .hidden .content, 
        .noscrollbar .content {
            overflow : hidden;
        }
        
        .bardown .borderRight {
            background  : url("images/error_box_r.png") repeat-y top right;
            overflow    : hidden;
            position    : absolute;
            right       : 0;
            top         : 0;
            bottom      : 26px;
            width       : 9px;
        }
        
        .bardown .bottomLeft {
            background : url("images/error_box_lb.png") no-repeat top left;
            width      : 8px;
            height     : 16px;
            float      : left;
            margin     : 0;
            _margin    : 0 -8px 0 0; /* IE6 bug fix */
            padding    : 0;
            border     : 0;
        }
        
        .bardown .bottomRight {
            background : url("images/error_box_rb.png") no-repeat top right;
            width      : 14px;
            height     : 16px;
            float      : right;
            margin     : 0;
            _margin    : 0 0 0 -14px; /* IE6 bug fix */
            padding    : 0;
            border     : 0;
        }
        
        .bardown .bottomBar {
            background : url("images/error_box_b.png") repeat-x bottom left;
            height     : 16px;
            margin     : 0 14px 0 8px;
            padding    : 0;
            border     : 0;
        }
        
        .bardown .close {
            width      : 13px;
            height     : 13px;
            background : url("images/close.png") no-repeat top left;
            margin     : -2px 1px 0 0;
            overflow   : hidden;
            position   : absolute;
            right      : 14px;
            top        : 9px;
        }
        
        .bardown .title .hover {
            background-position : 0 -13px;
        }
    ]]></a:style>

    <a:presentation>
        <!--  min-width="50" min-height="50"  -->
        <a:main collapsed-height="30" container="div[2]">
            <div class="bardown">
                <div class="borderRight"> </div>
                <div class="content">
                </div>
                <div class="bottomLeft"> </div>
                <div class="bottomRight"> </div>
                <div class="bottomBar"> </div>
            </div>
        </a:main>
        <a:button>
            <div> </div>
        </a:button>
        <a:cover>
            <div class='cover'> </div>
        </a:cover>
    </a:presentation>
</a:bar>
