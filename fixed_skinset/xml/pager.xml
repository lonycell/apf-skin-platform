        }
        
        .searchnav span:hover, 
        .searchnav .current {
            color        : white;
            border-color : white;
        }
        
        .pager .next {
        
        }
        
        .pager .previous {
        
        }
        
        .pager span.disabled,
        .pager span.disabled:hover {
            border  : 0;
            padding : 3px 5px 3px 4px;
            color   : #d7d7d7;
            cursor  : default;
        }
        
        .searchnav span.disabled,
        .searchnav span.disabled:hover {
            color : #999;
        }
    ]]></a:style>
    <a:style condition="apf.isSafari">
        .pager span{
            padding : 3px 5px 3px 4px;
        }
        .pager span:hover, 
        .pager .current {
