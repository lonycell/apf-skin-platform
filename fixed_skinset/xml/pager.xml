        <a:main container=".">
            <div class="pager">
            </div>
        </a:main>
        <a:button caption="text()">
            <span>-</span>
        </a:button>
    </a:presentation>
</a:pager>

<a:pager name="pgrnormal">
    <a:style><![CDATA[
        .pgrnormal {
            font-size     : 9pt;
            color         : #aaa;
            text-align    : center;
            line-height   : 10px;
            height        : 15px;
            padding       : 5px 0 7px 0;
            border-bottom : 1px solid #eee;
        }
        
        .btmpager {
            border-top    : 1px solid #eee;
            border-bottom : 0;
            padding       : 15px 0 0 0;
        }
        
        .pgrnormal div {
            margin : 0 auto;
            width  : 540px;
        }
        
        .pgrnormal span {
            padding        : 3px 6px 3px 6px;
            width          : 20px;
            height         : 18px;
            font-weight    : bold;
            margin-right   : 3px;
            cursor         : hand;
            text-align     : center;
            letter-spacing : 0;
            color          : #2c78bd;
            position       : relative;
            border         : 1px solid white;
        }
        
        .pgrnormal span:hover, 
        .pgrnormal .current {
            border          : 1px solid #ccc;
            color           : gray;
            text-decoration : none;
        }
        
        .pgrnormal .next {
        
        }
        
        .pgrnormal .previous {
        
        }
    ]]></a:style>

    <a:presentation>
        <a:main container="div">
            <div class="pgrnormal">
                <div>
                </div>
            </div>
        </a:main>
        <a:button caption="text()">
            <span>-</span>
        </a:button>
    </a:presentation>
</a:pager>