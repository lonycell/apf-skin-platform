<a:frame name="frame">
    <a:alias>fieldset</a:alias>
    
    <a:style><![CDATA[
        .frame {
            padding     : 0 7px 7px 7px;
            border      : 1px solid #dcd8d8;
            color       : #4b4b4b;
            font-family : Tahoma;
            font-size   : 8pt;
            /*position  : relative;
            display     : block;
            margin      : 13px 0 0 0;*/
            margin      : 0 0 5px 0;
            cursor      : default;
        }

        .frame .legend {
            /*margin-top : 0px;
            position     : absolute;
            background   : #fafbfc;
            left         : 5px;
            top          : -8px;
            padding      : 0 2px 0 2px;
            margin       : 0px;*/
            margin : 0 0 7px -7px;
            cursor : default;
        }
    ]]></a:style>
    <a:style condition="apf.isIE6">
        .frame {
            height : 1%;
        }
    </a:style>
    <a:style condition="!apf.isIE">
        .frame .legend {
            margin : 5px 0 7px -3px;
        }
    </a:style>
    <a:style condition="apf.isIE8">
        .frame .legend {
            margin : -2px 0 7px 0;
        }
    </a:style>
    
    <a:presentation>
        <a:main container="." caption="legend/text()">
            <fieldset class='frame'>
                <legend class="legend">-</legend>
            </fieldset>
        </a:main>
    </a:presentation>
</a:frame>