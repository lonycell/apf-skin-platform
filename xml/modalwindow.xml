<a:modalwindow name="dockwin">
    <a:alias>panel</a:alias>
    
    <a:style><![CDATA[
        .dockwin {
            font-family : Tahoma;
            font-size   : 11px;
            padding     : 21px 0 1px 0;
            position    : relative;
        }
    
        .dockwinFocus {
        
        }
        
        .dockwin .docktitle {
            background   : url("images/window_mt.png") repeat-x left 50%;
            height       : 17px;
            color        : white;
            position     : relative;
            margin       : 0;
            padding      : 2px 0 0 5px;
            border       : 1px solid black;
            border-width : 1px 1px 1px 1px;
            font-weight  : bold;
            margin-top   : -21px;
            cursor       : default;
            white-space  : nowrap;
            overflow     : hidden;
        }
        
        .dockwin .dockcontent {
            background   : #f5f5f5;
            overflow     : hidden;
            position     : relative;
            height       : 100%;
            border       : 1px solid #c3c3c3;
            border-width : 0 1px 1px 1px; 
        }
        
        .noborder .dockcontent {
            border-width : 0;
        }
        
        .dockwinMin .dockcontent {
            overflow : hidden;
        }
        
        .dockwin .docktitle blockquote {
            margin     : 0;
            width      : 49px;
            position   : absolute;
            padding    : 1px 1px 2px 0;
            top        : 0;
            right      : 0;
            background : url("images/window_mt.png") repeat-x left -4px;
        }
    
        .dockwin .docktitle blockquote div {
            width      : 10px;
            height     : 9px;
            float      : right;
            padding    : 3px;
            background : no-repeat 3px 3px;
            cursor     : default;
            _overflow  : hidden; /* IE6 bug fix */
        }
        
        .dockwin .docktitle blockquote div.hover {
            border              : 1px solid white;
            padding             : 2px;
            background-position : 2px -10px;
            height              : 10px;
            background-color    : transparent;
        }
        
        .dockwin .docktitle blockquote div.down {
            background-color : black;
        }
        
        .dockwin .docktitle .max {
            background-image : url(images/win_max.gif);
        }
        
        .dockwinMax .docktitle .max {
            background-image : url(images/win_restore.gif);
        }
        
        .dockwin .docktitle .min {
            background-image : url(images/win_min.gif);
        }
        
        .dockwinMin .docktitle .min {
            background-image : url(images/win_restore.gif);
        }
        
        .dockwin .docktitle .close {
            width            : 11px;
            background-image : url(images/win_x.gif);
        }
        
        .dockwin .docktitle span {
            width   : 16px;
            height  : 16px;
            display : block;
        }
        
        body .whitebg .content {
            background : white;
        }
        
        .north{cursor : url(images/cursor/n-point.cur);}
        .west{cursor : url(images/cursor/w-point.cur);}
        .east{cursor : url(images/cursor/e-point.cur);}
        .south{cursor : url(images/cursor/s-point.cur);}
        .same{cursor : not-allowed;}
    ]]></a:style>

    <a:presentation>
        <a:main 
          collapsed-height = "22"
          min-width        = "150" 
          min-height       = "80" 
          caption          = "div[1]/text()"
          container        = "div[2]" 
          drag             = "div[1]"
          title            = "div[1]/text()" 
          buttons          = "div[1]/blockquote">
            <div class="dockwin">
                <div class="docktitle">-
                    <blockquote> </blockquote>
                </div>
                <div class="dockcontent"> </div>
            </div>
        </a:main>
        <a:button>
            <div> </div>
        </a:button>
        <a:cover>
            <div class='cover'> </div>
        </a:cover>
    </a:presentation>
</a:modalwindow>

