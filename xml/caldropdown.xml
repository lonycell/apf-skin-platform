<a:caldropdown name="caldropdown">
    <a:style><![CDATA[
        .caldropdown {
            position    : relative;
            background  : url("images/dropdown.png") no-repeat top right;
            width       : 134px;
            height      : 17px;
            margin      : 0;
            padding     : 2px 18px 0 0;
            color       : #4b4b4b;
            font-family : Tahoma;
            font-size   : 11px;
        }

        .caldropdownError {
            background-position : right -57px;
        }

        .caldropdownDisabled {
            color : #bebebe;
        }

        .caldropdownFocus {
            background-position : right -19px;
        }

        .caldropdownOver {

        }

        .caldropdownDown {
            position            : relative;
            background-position : right -38px;
        }

        .caldropdown .caldropdownLeftBorder {
            background : url("images/dropdown.png") no-repeat top left;
            float      : left;
            margin     : -2px 0 0 0;
            width      : 2px;
            _margin    : -2px -1px 0 0;
            height     : 19px;
        }

        .caldropdown .caldropdownlabel {
            padding : 0 0 0 4px;
            cursor  : default;
            height  : 15px;
            margin  : 0 0 0 2px;
        }

        .caldropdownError .caldropdownLeftBorder {
            background-position : 0 -57px;
        }

        .caldropdownError .caldropdownlabel {
            background-color : #ffb500;
            color            : #fbfbfb;
        }

        .caldropdownInitial .caldropdownLeftBorder {
        }

        .caldropdownInitial .caldropdownlabel {
            color : #999;
        }

        .caldropdownInitial.caldropdownFocus .caldropdownLeftBorder {
            background-position : 0 -19px;
        }

        .caldropdownInitial.caldropdownFocus .caldropdownlabel {
            background-color : transparent;
            color            : #ffffff;
        }

        .caldropdownFocus .caldropdownLeftBorder {
            background-position : 0 -19px;
        }

        .caldropdownFocus .caldropdownlabel {
            background-color : #25a8e7;
            color            : #ffffff;
        }

        .caldropdownDown .caldropdownLeftBorder {
            background-position : 0 -38px;
        }

        .caldropdownDown .caldropdownlabel {
            color            : #4b4b4b;
            background-color : white;
        }

        .calSlider {
            position         : absolute;
            top              : 0;
            left             : 0;
            width            : 120px;
            margin-top       : -1px;
            border           : 1px solid #327fbd;
            background-color : white;
            z-index          : 1000;
            color            : #0d5381;
            font-family      : Tahoma;
            font-size        : 11px;
            display          : none;
            overflow         : auto;
        }

        .calSlider .navigation {
            display          : block;
            margin           : 0 auto;
            height           : 16px;
            padding          : 4px 3px 2px 3px;
            background-color : white;
        }

        .calSlider .navigation .button {
            width    : 13px;
            height   : 13px;
            position : relative;
            color    : #4b4b4b;
            z-index  : 1000;
        }
        
        .calSlider .navigation .nextMonth,
        .calSlider .navigation .prevMonth,
        .calSlider .navigation .nextYear,
        .calSlider .navigation .prevYear,
        .calSlider .navigation .today {
            width               : 13px;
            height              : 17px;
            padding-top         : 3px;
            position            : absolute;
            background-position : 50% 50%;
            background-repeat   : no-repeat;
            text-align          : center;
            cursor              : pointer;
        }
        
        .calSlider .navigation .nextMonth:hover,
        .calSlider .navigation .prevMonth:hover,
        .calSlider .navigation .nextYear:hover,
        .calSlider .navigation .prevYear:hover,
        .calSlider .navigation .today:hover {
            background-color : #ebebeb;
        }
        
        .calSlider .navigation .status {
            width       : 100px;
            padding-top : 3px;
            height      : 17px;
            position    : absolute;
            margin-left : -50px;
            top         : 1px;
            left        : 50%;
            text-align  : center;
            z-index     : 900;
        }
        
        .calSlider .navigation .nextMonth {
            right            : 19px;
            top              : 1px;
            background-image : url(images/calendar_m_plus2.png);
        }
    
        .calSlider .navigation .prevMonth {
            left             : 19px;
            top              : 1px;
            background-image : url(images/calendar_m_minus2.png);
        }
    
        .calSlider .navigation .nextYear {
           right            : 3px;
           top              : 1px;
           background-image : url(images/calendar_y_plus2.png);
        }
    
        .calSlider .navigation .prevYear {
            left             : 3px;
            top              : 1px;
            background-image : url(images/calendar_y_minus2.png);
        }
        
        .calSlider .navigation .today {
            right : 35px;
            top   : 1px;
        }

        .calSlider .daysofweek {
            margin   : 0 auto;
            display  : block;
            position : relative;
        }

        .calSlider .daysofweek .dayofweek {
            border              : 1px solid #f0f0f0;
            float               : left;
            font-family         : Tahoma;
            font-size           : 11px;
            color               : #4b4b4b;
            display             : block;
            margin              : 1px;
            text-align          : center;
            background-color    : #f0f0f0;
            cursor              : pointer;
            white-space         : nowrap
            overflow            : hidden;
        }

        .calSlider .row {
            margin   : 0 auto;
            display  : block;
            position : relative;
            padding  : 0 1px 0 1px;
        }

        .calSlider .row .cell {
            position            : relative;
            border              : 1px solid #b2b2b2;
            float               : left;
            display             : block;
            text-align          : center;
            background-color    : white;
            cursor              : pointer;
            white-space         : nowrap;
            border              : 1px solid white;
            color               : #4b4b4b;
        }

        .calSlider .row .weekend {
            color : #ed1c24;
        }

        .calSlider .row .disabled {
            color            : #e1e1e1;
            background-color : white;
        }
        
        .calSlider .row .weeknumber {
            background-color : #f0f0f0;
            border           : 1px solid #f0f0f0;
        }

        .calSlider .row .hover {
            background-color : #25a8e7;
            color            : white;
            border           : 1px solid #25a8e7;
        }

        .calSlider .row .active {
            background-color : #25a8e7;
            color            : white;
            border           : 1px solid #25a8e7;
        }
    ]]></a:style>
    <a:style condition="apf.isGecko"><![CDATA[
        .calSlider .row .cell {
            float   : none;
            display : -moz-grid;
        }
    ]]></a:style>

    <a:presentation>
        <a:main label="div[2]" button=".">
            <div class='caldropdown'>
                <div class='caldropdownLeftBorder'> </div>
                <div class='caldropdownlabel'>-</div>
            </div>
        </a:main>
        <a:container contents="." navigation="div[1]" daysofweek="div[2]">
            <div class='calSlider'>
                <div class="navigation"> </div>
                <div class="daysofweek"> </div>
            </div>
        </a:container>
        <a:cell>
            <div class="cell"> </div>
        </a:cell>
        <a:row>
            <div class="row"> </div>
        </a:row>
        <a:day>
            <div class="dayofweek"> </div>
        </a:day>
        <a:button>
            <div class="button"> </div>
        </a:button>
        <a:empty caption=".">
            <div class="empty">-</div>
        </a:empty>
    </a:presentation>
</a:caldropdown>