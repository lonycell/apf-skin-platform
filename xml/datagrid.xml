<a:skin xmlns:a="http://ajax.org/2005/aml">
    <a:datagrid name="datagrid">
        <a:style><![CDATA[
            .datagrid {
                position         : relative;
                border           : 1px solid #c3c3c3;
                background-color : white;
                cursor           : default;
                font-family      : Tahoma;
                font-size        : 8pt;
                padding          : 21px 0 0 0;
                color            : #333;
            }
    
            .datagrid .headings {
                position         : relative;
                top              : 0;
                left             : 0;
                height           : 17px;
                z-index          : 10;
                /*
                padding-left : 5px;
                */
                background-color : #e6e6e6;
                margin           : -21px 17px 0 0;
                border           : 1px solid white;
                border-left      : 0;
                border-right     : 0px solid white;
                white-space      : nowrap;
            }
            .noscrollbar .headings {
                margin : -21px 1px 0 0;
            }
            .noscrollbar .datagrid {
                background : transparant;
            }
    
            .datagrid .headings DIV {
                display       : inline-block;
                height        : 17px;
                /*
                margin-right : -3px;
                margin-left  : -5px;
                */
                background    : #e6e6e6 no-repeat 0 50%;
                overflow      : hidden;
                text-overflow : ellipsis;
            }
            .datagrid .headings DIV U {
                padding         : 1px 4px 1px 3px;
                display         : inline-block;
                height          : 15px;
                text-decoration : none;
                border-left     : 1px solid white;
            }
    
            .datagrid .headings DIV.hover {
                background-color: #DDD;
            }
    
            .datagrid .headings DIV.down, 
            .datagrid .headings DIV.drag {
                color            : white;
                background-color : #25a8e7;
            }
    
            .datagrid .headings DIV.drag {
                border : 1px solid white;
            }
    
            .datagrid .headings DIV.ascending {
                background-image    : url(images/sort_asc.gif);
                background-repeat   : no-repeat;
                background-position : right 6px;
            }
    
            .datagrid .headings DIV.descending {
                background-image    : url(images/sort_desc.gif);
                background-repeat   : no-repeat;
                background-position : right 6px;
            }
    
            .datagrid .records {
                overflow-x   : hidden;
                overflow-y   : scroll;
                height       : 100%;
                padding      : 18px 0 0 0;
                position     : relative;
                top          : -19px;
                border       : 1px solid white;
                border-width : 1px 1px 0 0;
                white-space  : nowrap;
            }
            
            .noscrollbar .records {
                overflow-y : hidden;
            }
    
            .datagrid .records .row SPAN {
                display       : inline-block;
                /*
                padding : 1px 0 1px 0;
                */
                height        : 18px;
                background    : white no-repeat 0 50%;
                border-bottom : 1px solid #eee;
            }
            
            .datagrid .records .row SPAN U {
                padding         : 2px 3px 2px 4px;
                height          : 15px;
                display         : inline-block;
                text-decoration : none;
                border-left     : 1px solid #eee;
                text-overflow   : ellipsis;
                white-space     : nowrap;
                overflow        : hidden;
            }
    
            .noscrollbar .datagrid .records .row SPAN {
                background-color : transparent;
            }
    
            .datagrid .records .row {
                height       : 19px;
                padding-left : 0;
            }
    
            .datagrid .records .highlight SPAN {
    
            }
    
            .datagrid .records .highlight {
    
            }
    
            .datagridFocus .records .indicate SPAN {
                /*
                border       : 1px dotted #BBB;
                border-width : 1px 0 1px 0;
                */
                padding : 0;
                color   : #333;
            }
            
            .datagridFocus .records .indicate SPAN U {
            }
    
            .datagridFocus .records .row SPAN {
            }
    
            .datagrid .records .selected, 
            .datagrid .records .selected SPAN {
                background-color : #f0f0f0;
                color            : #333;
            }
            
            .datagridFocus .records .selected, 
            .datagridFocus .records .selected SPAN {
                color            : white;
                background-color : #25a8e7;
            }
            
            .datagrid .records .editing, 
            .datagrid .records .editing SPAN {
                background-color : white;
                color            : #333;
            }
    
            .datagrid .move_pointer {
                height     : 100px;
                width      : 2px;
                position   : absolute;
                top        : 0;
                margin     : -10px 0 0 -4px;
                width      : 9px;
                height     : 38px;
                background : url(images/column_picker.gif) no-repeat 0 0;
                overflow   : hidden;
                z-index    : 1000;
            }
    
            .datagrid .size_pointer {
                border       : 1px dotted gray;
                border-width : 0 1px 0 1px;
                height       : 100%;
                position     : absolute;
                top          : 0px;
                z-index      : 1000;
                cursor       : w-resize;
            }
    
            .datagrid .message {
                font-weight   : normal;
                color         : #333;
                padding       : 0 0 10px 50px;
                font-size     : 11px;
                margin-top    : 10px;
                /*
                text-align : center;
                */
                border-bottom : 1px solid #eee;
            }
            
            .datagrid .message LABEL {
                padding  : 2px 0 0 4px;
                height   : 14px;
                display  : inline-block;
                position : relative;
                top      : -3px;
            }
            
            /*.datagrid .message SPAN {
                background : url(icons/cog.png) no-repeat left bottom;
                width      : 16px;
                height     : 16px;
                display    : inline-block;
            }*/
            
            .datagrid .loading, 
            .datagrid .empty, 
            .datagrid .offline {
                padding : 5px;
                margin  : 0;
            }
            
            .datagrid .loading SPAN, 
            .datagrid .empty SPAN, 
            .datagrid .offline SPAN {
                background : white;
                display : inline;
            }
    
            .pointer {
                display : none;
            }
    
            .dginfo {
                display          : none;
                overflow         : hidden;
                white-space      : normal;
                padding          : 5px;
                background-color : #f3f3f3;
                margin-top       : 3px;
                margin-left      : 6px;
                position         : relative;
                left             : -3px;
                margin-bottom    : 3px;
            }
    
            .dginfo H3 {
                margin : 0 0 5px 0;
            }
    
            .dginfo P {
                margin : 0;
            }
    
            .datagrid .records .stdel, 
            .datagridFocus .records .stdel,
            .datagrid .records .stdel SPAN, 
            .datagridFocus .records .stdel SPAN {
                color : #ccc;
            }
    
            .dragdg {
                border : 2px solid black;
                width  : 10px;
                height : 5px;
            }
    
            .datagrid .records .dragInsert {
                border-top : 2px solid gray;
                height     : 17px;
            }
    
            .datagrid .records .dragInsert SPAN {
                padding-top         : 0px;
                margin-bottom       : 0;
                background-position : 0 0;
            }
            
            .datagrid BLOCKQUOTE {
                padding    : 0;
                margin     : 0;
                display    : none;
                height     : 0;
                overflow   : hidden;
                background : repeat-y 0px center;
            }
            
            .datagrid .records SPAN.treecell U {
                text-decoration : none;
                display         : inline-block;
                padding         : 1px 2px 3px 2px;
                border          : 0;
            }
            
            .datagrid .treecell STRONG {
                width    : 9px;
                height   : 9px;
                margin   : 4px 4px 0 4px;
                position : relative;
                top      : -5px;
                display  : inline-block;
            }
            
            .datagrid .plus>.treecell STRONG,
            .datagrid .pluslast>.treecell STRONG {
                background-image : url(images/plus.png);
            }
            
            .datagrid .min>.treecell STRONG,
            .datagrid .minlast>.treecell STRONG {
                background-image : url(images/min.png);
            }
            
            .datagrid .slider {
                margin : 5px 5px 0 3px;
            }
            
            .datagrid .tb {
                border      : 0;
                padding     : 2px 0 4px 4px;
                background  : white;
                border-left : 1px solid #eee;
                height      : 13px;
                position    : relative;
                top         : -6px;
            }
        ]]></a:style>
        <a:style condition="apf.isIE &amp;&amp; !apf.isIE8"><![CDATA[
            .datagrid .headings DIV {
                float : left;
            }
    
            .datagrid .records .row SPAN {
                float : left;
            }
        ]]></a:style>
        <a:style condition="apf.isGecko &amp;&amp; apf.versionGecko &lt; 3"><![CDATA[
            .datagrid .headings {
                padding-top : 2px;
                height      : 15px;
            }
    
            .datagrid .headings DIV {
                display  : -moz-inline-box;
                overflow : visible;
                position : relative;
            }
    
            .datagrid .records {
            }
    
            .datagrid .records .row SPAN {
                display  : -moz-inline-box;
                overflow : visible;
                position : relative;
            }
        ]]></a:style>
    
        <a:presentation>
            <a:main
                head        = "div[1]"
                container   = "div[3]"
                pointer     = "div[2]"
                widthdiff   = "1"
                minwidth    = "210"
                minheight   = "40"
                iframe2     = "true">
                <div class="datagrid">
                    <div class='headings'>
    
                    </div>
                    <div class='pointer'> </div>
                    <div class='records'> </div>
                </div>
            </a:main>
    
            <a:headitem class="." caption="u/text()">
                <div><u>-<span></span></u></div>
            </a:headitem>
    
            <a:rowheaditem class="." caption="text()">
                <span>-</span>
            </a:rowheaditem>
    
            <a:item class="." container="following-sibling::blockquote">
                <div class="row"></div>
                <blockquote> </blockquote>
            </a:item>
    
            <a:dragindicator>
                <div class='dragdg'> </div>
            </a:dragindicator>
    
            <a:cell caption="u"><span><u>-</u></span></a:cell>
            
            <a:treecell caption="u" openclose="strong"><span class="treecell"><strong> </strong><u>-</u></span></a:treecell>
    
            <a:container container=".">
                <div class="dginfo">-</div>
            </a:container>
    
            <a:loading>
                <div class="message"><span></span><label>Calculating...</label></div>
            </a:loading>
    
            <a:empty caption=".">
                <div class="empty"> </div>
            </a:empty>
        </a:presentation>
    </a:datagrid>
</a:skin>