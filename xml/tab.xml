<a:tab name="demos_lowtab">
    <a:style><![CDATA[
        .demos_tab {
           color       : #4b4b4b;
           font-family : Tahoma;
           font-size   : 8pt;
           padding     : 23px 0 0 0;
           position    : relative;
           z-index     : 100;
        }
    
        .demos_tab .demobtn {
            padding-left : 0;
            margin-top   : -19px;
            position     : absolute;
            z-index      : 100;
        }
    
        .demos_tab .demobtn div {
            cursor  : default;
            display : block;
            float   : left;
            margin  : 0 3px 0 0;
            padding : 0;
        }
    
        .demos_tab .demobtn div {
            background : url(images/tab_but_topleft.png) no-repeat 0 0;
            height     : 20px;
        }
    
        .demos_tab .demobtn div.curbtn {
            _position           : relative;
            margin-top          : -4px;
            margin-bottom       : -1px;
            background-position : 0 -19px;
            height              : 24px;
            color               : #327fbd;
        }
    
        .demos_tabFocus .curbtn {
        }
    
        .demos_tabFocus .curbtn span {
            border  : 1px dotted gray;
            padding : 0px 3px 2px 3px;
            margin  : -1px -4px 0 -4px;
            display : block;
        }
    
        .demos_tab .demobtn div div {
            height  : 16px;
            margin  : 0 -4px 0 0;
            padding : 4px 8px 0 8px;
        }
    
        .demos_tab .demobtn div div {
            background : url(images/tab_but_topright.png) no-repeat right 0;
        }
    
        .demos_tab .demobtn div.curbtn div {
            background-position : right -19px;
            height              : 24px;
            padding-top         : 5px;
        }
    
        .demos_tab .demos_tabpage {
            clear         : both;
            border-top    : 1px solid #d7d7df;
            border-left   : 1px solid #c3c3c3;
            border-right  : 1px solid #c3c3c3;
            border-bottom : 1px solid #c3c3c3;
            background    : #fafbfc;
            padding       : 7px;
            overflow      : hidden;
            display       : none;
            height        : 100%;
            position      : relative;
            z-index       : 99;
        }
    
        .demos_tab .demos_tabpage.curpage {
            display : block;
        }
    ]]></a:style>
    <a:style condition="apf.isIE6">
        .demos_tab .demobtn div, 
        .demos_tab .demobtn div div {
            margin : 0 0 0 0;
        }
        .demos_tab .demos_tabFooter {
            bottom : 11px;
        }
    </a:style>
    <a:presentation>
        <a:main pages="." buttons="div">
            <div class="demos_tab">
                <div class="demobtn">

                </div>
            </div>
        </a:main>
        <a:button caption="div/span/text()">
            <div class="btn">
                <div>
                    <span>-</span>
                </div>
            </div>
        </a:button>
        <a:page container="." inherit="true">
            <div class="demos_tabpage"> </div>
        </a:page>
    </a:presentation>
</a:tab>

<a:tab name="tab">
    <a:style><![CDATA[
        .tab {
            color       : #4b4b4b;
            font-family : Tahoma;
            font-size   : 8pt;
            padding     : 23px 0 26px 0;
            overflow    : hidden;
            position    : relative;
        }
    
        .tab .btncontainer {
            padding-left : 0;
            margin-top   : -19px;
            position     : absolute;
            z-index      : 100;
            width        : 100000px;
            height       : 20px;
            overflow     : visible;
        }
    
        .tab .btncontainer div {
            cursor  : default;
            display : block;
            float   : left;
            margin  : 0 3px 0 0;
            padding : 0;
        }
    
        .tab .btncontainer div {
            background : url(images/tab_but_topleft.png) no-repeat 0 0;
            height     : 20px;
        }
        
        .tab .btncontainer div span {
            float : left;
        }
        
        .tab .btncontainer div .btnClose {
            width      : 9px;
            height     : 9px;
            position   : relative;
            background : url(images/btnCloseTab.png) 0 0  no-repeat;
            margin     : 3px 0 0 6px;
            cursor     : pointer;
        }
        
        .tab .btncontainer div .btnClose:hover {
            background-position : 0 -9px;
        }
    
        .tab .btncontainer div.curbtn {
            _position           : relative;
            margin-top          : -4px;
            margin-bottom       : -1px;
            background-position : 0 -19px;
            height              : 24px;
            color               : #327fbd;
        }
        
        .tabFocus .curbtn {
        }
    
        .tabFocus .curbtn span {
            border  : 1px dotted gray;
            padding : 0px 3px 2px 3px;
            margin  : -1px -4px 0 -4px;
            display : block;
        }
    
        .tab .btncontainer div div {
            height  : 16px;
            margin  : 0 -4px 0 0;
            padding : 4px 8px 0 8px;
        }
    
        .tab .btncontainer div div {
            background : url(images/tab_but_topright.png) no-repeat right 0;
        }
    
        .tab .btncontainer div.curbtn div {
            background-position : right -19px;
            height              : 24px;
            padding-top         : 5px;
        }
        
        .tabDisabled .btncontainer div span {
            color : #bebebe;
        }
        
        .tabDisabled .btncontainer .curbtn {
            border        : 0;
            margin-top    : -4px;
            margin-bottom : -1px;
            padding       : 0;
        }
    
        .tab .page {
            clear        : both;
            border-top   : 1px solid #d7d7df;
            border-left  : 1px solid #c3c3c3;
            border-right : 1px solid #c3c3c3;
            background   : #fafbfc;
            padding      : 7px;
            overflow     : hidden;
            display      : none;
            height       : 100%;
            position     : relative;
        }
    
        .tab .page.curpage {
            display : block;
        }
    
        .tab .tabFooter {
            clear      : both;
            height     : 15px;
            background : url(images/tab_pag_lb.png) no-repeat bottom left;
            position   : absolute;
            bottom     : 0;
            width      : 100%;
        }
    
        .tab .tabFooter div {
            background : url(images/tab_pag_rb.png) no-repeat bottom right;
        }
    
        .tab .tabFooter div div {
            border-bottom    : 1px solid #c3c3c3;
            background-color : #fafbfc;
            background-image : none;
            height           : 14px;
            margin           : 0 14px;
        }
    
        .tab .scroller {
            position : absolute;
            top      : 7px;
            right    : 0px;
            width    : 32px;
            margin   : 0;
            padding  : 0;
            z-index  : 101;
        }
    
        .tab .scroller .scroller_btn_left,
        .tab .scroller .scroller_btn_right {
            position            : relative;
            float               : left;
            width               : 15px;
            height              : 17px;
            background-image    : url(images/tab_scroller_btns.gif);
            background-position : 0 0;
            background-repeat   : no-repeat;
            margin              : 0;
            padding             : 0;
        }
    
        .tab .scroller .scroller_btn_right {
            float               : right;
            width               : 17px;
            background-position : -15px 0;
        }
    ]]></a:style>
    
    <a:style condition="apf.isIE6"><![CDATA[
        .tab .btncontainer div, 
        .tab .btncontainer div div {
            margin : 0 0 0 0;
        }
        .tab .tabFooter {
            bottom : 11px;
        }
    ]]></a:style>
    <a:style condition="!apf.isIE"><![CDATA[
        .tab .scroller .scroller_btn_left.hover {
            background-position : 0 -17px;
        }
    
        .tab .scroller .scroller_btn_left.down {
            background-position : 0 -34px;
        }
    
        .tab .scroller .scroller_btn_left.disabled {
            background-position : 0 -51px;
        }
    
        .tab .scroller .scroller_btn_right.hover {
            background-position : -15px -17px;
        }
    
        .tab .scroller .scroller_btn_right.down {
            background-position : -15px -34px;
        }
    
        .tab .scroller .scroller_btn_right.disabled {
            background-position : -15px -51px;
        }
    ]]></a:style>
    <a:style condition="apf.isIE"><![CDATA[
        .tab .scroller span.hover {
            background-position-y : -17px;
        }
    
        .tab .scroller span.down {
            background-position-y : -34px;
        }
    
        .tab .scroller span.disabled {
            background-position-y : -51px;
        }
    ]]></a:style>
    <a:presentation>
        <a:main 
          pages    = "." 
          buttons  = "div[1]" 
          scroller = "div[2]">
            <div class="tab">
                <div class="btncontainer">

                </div>

                <div class="scroller">
                    <span class="scroller_btn_left"> </span>
                    <span class="scroller_btn_right"> </span>
                </div>

                <div class="tabFooter">
                    <div>
                        <div> </div>
                    </div>
                </div>
            </div>
        </a:main>
        <a:button caption="div/span/text()" btncontainer="div">
            <div class="btn">
                <div>
                    <span>-</span>
                </div>
            </div>
        </a:button>
        <a:page container="." inherit="true">
            <div class="page"> </div>
        </a:page>
        <a:btnclose container=".">
            <span class="btnClose"> </span>
        </a:btnclose>
    </a:presentation>
</a:tab>

<a:tab name="lowtab">
    <a:style><![CDATA[
        .lowtab {
            position    : relative;
            color       : #4B4B4B;
            font-family : Tahoma;
            font-size   : 8pt;
            padding     : 0 0 26px 0;
        }
    
        .lowtab .btncontainer {
            position : absolute;
            bottom   : -14px;
            /* _bottom : 1px;*/
            left     : 10px;
            cursor   : default;
            z-index  : 1;
        }
        
        .lowtab .btncontainer div {
            cursor     : default;
            display    : block;
            float      : left;
            margin     : 0 3px 0 0;
            /*_margin : 0 0 0 0;*/
            padding    : 0;
            background : url(images/tab_but_bottomleft.png) no-repeat 0 0;
            height     : 20px;
        }
    
        .lowtab .btncontainer .curbtn {
            background-position : 0 -20px;
            height              : 25px;
            color               : #327fbd;
            position            : relative;
        }
    
        .lowtab .btncontainer div div {
            margin     : 0 -4px 0 0;
            /*_margin : 0 0 0 0;*/
            padding    : 2px 8px 0 8px;
            height     : 17px;
            border-top : 1px solid #c3c3c3;
            background : url(images/tab_but_bottomright.png) no-repeat right -1px;
        }
        
        .lowtab .btncontainer .curbtn div {
            padding-top : 6px;
            height      : 19px;
            border      : 0;
            background  : url(images/tab_but_bottomright.png) no-repeat right -20px;
        }
    
        .lowtab .page {
            background-color : #FAFBFC ;
            border-left      : 1px solid #C3C3C3;
            border-right     : 1px solid #C3C3C3;
            border-top       : 1px solid #D7D7DF;
            border-bottom    : 1px solid #D7D7DF;
            clear            : both;
            display          : none;
            height           : 100%;
            overflow         : hidden;
            padding          : 7px;
            position         : relative;
        }
    
        .lowtab .page.curpage {
            display : block;
        }
    ]]></a:style>
    <a:style condition="apf.isIE6">
        .lowtab .btncontainer {
            bottom : 1px;
        }
        .lowtab .btncontainer div {
            margin : 0 0 0 0;
        }
         .lowtab .btncontainer div div {
            margin : 0 0 0 0;
        }
    </a:style>
    <a:presentation>
        <a:main pages="." buttons="div">
            <div class='lowtab'>
                <div class='btncontainer'>
                </div>
            </div>
        </a:main>
        <a:button caption="div/span/text()">
            <div class="btn">
                <div>
                    <span>-</span>
                </div>
            </div>
        </a:button>
        <a:page container="." inherit="true">
            <div class='page'></div>
        </a:page>
    </a:presentation>
</a:tab>

<a:tab name="tablowms">
    <a:style><![CDATA[
        .lowtabms {
            width          : 300px;
            height         : 100px;
            padding-bottom : 24px;
            overflow       : hidden;
            position       : relative;
            background     : #808080;
        }
    
        .lowtabms .btncontainer {
            position   : absolute;
            bottom     : 0;
            padding    : 0 0 0 5px !important; padding: 0 0 3px 5px;
            background : url(images/tabdiv.gif) no-repeat 5px 4px;
            z-index    : 100;
        }
        
        * html .lowtabms .btncontainer {
            bottom : expression(this.parentNode.offsetHeight%2==1 ? 0: 1);
        }
    
        .lowtabms .btn {
            float       : left;
            display     : inline;
            padding     : 4px 8px 4px 8px;
            font-family : MS Sans Serif;
            font-size   : 8pt;
            color       : white;
            cursor      : default;
            position    : relative;
            z-index     : 90;
            background  : url(images/tabdiv.gif) no-repeat right 4px;
        }
    
        .lowtabms .curbtn {
            border-width : 0 1px 1px 1px;
            border-style : solid;
            border-color : black black black white;
            background   : #D4D0C8;
            color        : black;
            margin       : 2px 0 0 0;
            padding      : 3px 8px 3px 10px;
            margin       : 0 -2px 3px -2px;
            left         : 0px;
            z-index      : 100;
            position     : relative;
            top          : -1px;
        }
    
        * html .lowtabms .curbtn {
            top : 0;
        }
    
        .lowtabms .firstcurbtn {
            margin  : 0 -2px 3px 0;
            padding : 3px 8px 3px 8px;
        }
    
        .lowtabms .tabpage {
            /*border-width : 1px 0 1px 1px;
            border-style   : solid;
            border-color   : white black black white;*/
            display          : none;
            background-color : white;
        }
    
        .lowtabms .tabpage.curpage {
            display  : block;
            height   : 100%;
            overflow : hidden;
        }
    ]]></a:style>

    <a:presentation>
        <a:main pages="." buttons="div">
            <div class='lowtabms'>
                <div class='btncontainer'>
                </div>
            </div>
        </a:main>
        <a:button caption="text()">
            <span class='btn'>-</span>
        </a:button>
        <a:page container="." inherit="true">
            <div class='tabpage'></div>
        </a:page>
    </a:presentation>
</a:tab>