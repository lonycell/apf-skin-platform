<a:skin xmlns:a="http://ajax.org/2005/aml">
    <a:button name="buttonIconDown">
        <a:style><![CDATA[
            .apf_btnID {
                color       : #4b4b4b;
                font-family : Tahoma;
                font-size   : 8pt;
                height      : 25px;
                width       : 50px;
                overflow    : hidden;
                cursor      : default;
            }
        
            .apf_btnID .left {
                float         : left;
                width         : 8px;
                height        : 25px;
                _margin-right : -3px; /* IE6 bug fix */
            }
        
            .apf_btnID .lbl {
                height        : 20px;
                padding-top   : 5px;
                text-align    : center;
                margin        : 0;
                _margin       : 0; /* IE6 fix */
                white-space   : nowrap;
                overflow      : hidden;
                text-overflow : ellipsis;
                position      : relative;
            }
            
            .apf_btnID .lbl SPAN {
                padding             : 0;
                position            : absolute;
                top                 : 4px;
                left                : 0;
                border              : 0;
                margin              : 0;
                width               : 30px;
                height              : 16px;
                background-position : 0 0;
                background-repeat   : no-repeat;
            }
            
            .apf_btnID .lbl SPAN BLOCKQUOTE {
                background-position : 0 0;
                background-repeat   : no-repeat;
                background-image    : url(images/arrow_down.gif);
                width               : 7px;
                height              : 4px;
                position            : absolute;
                top                 : 7px;
                left                : 21px;
                margin              : 0;
                padding             : 0;
            }
        
            .apf_btnID .right {
                float       : right;
                width       : 9px;
                height      : 25px;
               _margin-left : -3px; /* IE6 bug fix */
            }
        
            .apf_btnID .left {
                background : url("images/button_left.png") no-repeat 0 -25px;
            }
        
            .apf_btnID .lbl {
                background : url("images/button_middle.png") repeat-x 0 -25px;
            }
        
            .apf_btnID .right {
                background : url("images/button_right.png") no-repeat 0 -25px;
            }
        
            .apf_btnIDDefault .left {
                background : url("images/button_left.png") no-repeat top left;
            }
        
            .apf_btnIDDefault .lbl {
                background : url("images/button_middle.png") repeat-x top left;
            }
        
            .apf_btnIDDefault .right {
                background : url("images/button_right.png") no-repeat top left;
            }
        
            .apf_btnIDDisabled  {
                color : #bebebe;
            }
        
            .apf_btnIDDisabled .left {
                background : url("images/button_left.png") no-repeat 0 -50px;
            }
        
            .apf_btnIDDisabled .lbl {
                background : url("images/button_middle.png") repeat-x 0 -50px;
            }
        
            .apf_btnIDDisabled .right {
                background : url("images/button_right.png") no-repeat 0 -50px;
            }
            .apf_btnIDFocus {
            }
        
            .apf_btnIDFocus .lbl SPAN {
            }
        
            .apf_btnIDDown .left {
                background : url("images/button_left.png") no-repeat 0 -75px;
            }
        
            .apf_btnIDDown .lbl {
                background : url("images/button_middle.png") repeat-x 0 -75px;
            }
        
            .apf_btnIDDown .right {
                background : url("images/button_right.png") no-repeat 0 -75px;
            }
            .apf_btnIDDown.redBtn .left {
                background-image : url("images/red_button_left.png");
            }
        
            .apf_btnIDDown.redBtn .lbl {
                background-image : url("images/red_button_middle.png");
            }
        
            .apf_btnIDDown.redBtn .right {
                background-image : url("images/red_button_right.png");
            }
            
            .apf_btnIDOver {
            }
        ]]></a:style>
        <a:style condition="apf.isIE7">
            .apf_btnID .right {
                margin : 0;
            }
        
            .apf_btnID .left {
                margin : 0;
            }
        </a:style>
    
        <a:presentation>
            <a:main 
              
              icon       = "div[3]/span" 
              background = ".">
                <div class="apf_btnID">
                    <div class="left"> </div>
                    <div class="right"> </div>
                    <div class="lbl">
                        <span>
                            <blockquote />
                        </span>
                    </div>
                </div>
            </a:main>
        </a:presentation>
    </a:button>
    
    <a:button name="toolbarbutton">
        <a:style><![CDATA[
            .menubar .toolbarbutton {
                display         : block;
                height          : 18px;
                float           : left;
                cursor          : default;
                padding         : 4px 8px 0 8px;
                line-height     : 13px;
                text-decoration : none;
                color           : #000000;
            }
            
            .menubar .toolbarbuttonOver, 
            .menubar .toolbarbuttonDown {
                background : #45b3e8 url(images/menu_over.gif);
                color      : #ffffff;
            }
            
            .menubar .toolbarbuttonDown, 
            .menubar .toolbarbuttonDown {
                background : #45b3e8 url(images/menu_down.gif);
                color      : #ffffff;
            }
            
            .menubar .toolbarbuttonDisabled, 
            .menubar .toolbarbuttonDisabled {
                background : none;
                color      : #bebebe;
            }
            
            .toolbar .subbar .toolbarbutton {
                display         : block;
                height          : 22px;
                float           : left;
                cursor          : default;
                margin          : 2px 1px 0 1px;
                text-decoration : none;
                color           : #000000;
                text-align      : center;
                padding         : 0 4px 0 0;
                line-height     : 13px;
            }
    
            .toolbar .subbar .toolbarbutton DIV {
                margin   : 0;
                height   : 17px;
                padding  : 4px 4px 0 8px;
                border   : 0;
                position : relative;
            }
            
            .toolbar .subbar .toolbarbuttonOver {
                background : url("images/toolbar_a_hover_right.png") no-repeat top right;
            }
            
            .toolbar .subbar .toolbarbuttonOver DIV {
                background : url("images/toolbar_a_hover.png") no-repeat top left;
            }
    
            .toolbar .subbar .toolbarbuttonDown,
            .toolbar .subbar .toolbarbuttonChecked {
                background : url("images/toolbar_a_down_right.png") no-repeat top right;
            }
            
            .toolbar .subbar .toolbarbuttonDown DIV, .toolbar .subbar .toolbarbuttonChecked DIV {
                background : url("images/toolbar_a_down.png") no-repeat top left;
            }
    
            .toolbar .subbar .toolbarbuttonDisabled, 
            .toolbar .subbar .toolbarbuttonDisabled DIV {
                background : none;
                color      : #bebebe;
            }
            
            .toolbar .subbar .toolbarbuttonIcon DIV {
                padding-left : 4px;
            }
            
            .toolbar .subbar .toolbarbuttonEmpty DIV {
                padding : 4px 0 0 7px;
            }
            
            .toolbar .subbar .toolbarbuttonIcon SPAN.icon {
                display : block;
                float   : left;
                width   : 16px;
                height  : 16px;
                margin  : -2px 4px 0 0;
            }
            
            .toolbar .subbar .toolbarbuttonIcon SPAN.caption {
                display : block;
                float   : left;
            }
            
            .toolbar .subbar .toolbarbuttonEmpty SPAN.icon {
                margin : -1px 3px 0 0;
            }
        ]]></a:style>
        
        <a:presentation>
            <a:main 
              caption    = "div/span[2]/text()" 
              background = "div/span" 
              icon       = "div/span"
              minheight  = "22" 
              minwidth   = "30"
              maxheight  = "22">
                <div class="toolbarbutton">
                    <div>
                        <span class="icon"></span>
                        <span class="caption">-</span>
                    </div>
                </div>
            </a:main>
        </a:presentation>
    </a:button>
    
    <a:button name="button">
        <a:alias>submit</a:alias>
        <a:alias>reset</a:alias>
        
        <a:style><![CDATA[
            .apf_btn {
                color       : #4b4b4b;
                font-family : Tahoma;
                font-size   : 8pt;
                height      : 25px;
                min-width   : 30px;
                min-height  : 25px;
                max-height  : 25px;
                overflow    : hidden;
                cursor      : default;
            }
        
            .apf_btn .left {
                float         : left;
                width         : 8px;
                height        : 25px;
                _margin-right : -3px; /* IE6 bug fix */
            }
        
            .apf_btn .lbl {
                height        : 20px;
                padding-top   : 5px;
                text-align    : center;
                margin        : 0;
                _margin       : 0; /* IE6 fix */
                white-space   : nowrap;
                overflow      : hidden;
                text-overflow : ellipsis;
            }
        
            .apf_btn .right {
                float       : right;
                width       : 9px;
                height      : 25px;
               _margin-left : -3px; /* IE6 bug fix */
            }
        
            .apf_btn .left {
                background : url("images/button_left.png") no-repeat 0 -25px;
            }
        
            .apf_btn .lbl {
                background : url("images/button_middle.png") repeat-x 0 -25px;
            }
        
            .apf_btn .right {
                background : url("images/button_right.png") no-repeat 0 -25px;
            }
        
            .apf_btnDefault .left {
                background : url("images/button_left.png") no-repeat top left;
            }
        
            .apf_btnDefault .lbl {
                background : url("images/button_middle.png") repeat-x top left;
            }
        
            .apf_btnDefault .right {
                background : url("images/button_right.png") no-repeat top left;
            }
        
            .redBtn {
                color : #ffffff;
            }
        
            .redBtn .left {
                background-image : url("images/red_button_left.png");
            }
        
            .redBtn .lbl {
                background-image : url("images/red_button_middle.png");
            }
        
            .redBtn .right {
                background-image : url("images/red_button_right.png");
            }
        
            .apf_btn span {
                padding  : 1px 3px 1px 3px;
                border   : 1px solid transparent;
                position : relative;
                _border  : 0;
                _padding : 2px 4px 2px 4px;
                _margin  : 0;
            }
        
            .apf_btnDisabled  {
                color : #bebebe;
            }
        
            .apf_btnDisabled .left {
                background : url("images/button_left.png") no-repeat 0 -50px;
            }
        
            .apf_btnDisabled .lbl {
                background : url("images/button_middle.png") repeat-x 0 -50px;
            }
        
            .apf_btnDisabled .right {
                background : url("images/button_right.png") no-repeat 0 -50px;
            }
            .apf_btnFocus {
            }
        
            .apf_btnFocus span {
                border  : 1px dotted #BBB;
                padding : 1px 3px 1px 3px;
            }
        
            .apf_btnDown .left {
                background : url("images/button_left.png") no-repeat 0 -75px;
            }
        
            .apf_btnDown .lbl {
                background : url("images/button_middle.png") repeat-x 0 -75px;
            }
        
            .apf_btnDown .right {
                background : url("images/button_right.png") no-repeat 0 -75px;
            }
            .apf_btnDown.redBtn .left {
                background-image : url("images/red_button_left.png");
            }
        
            .apf_btnDown.redBtn .lbl {
                background-image : url("images/red_button_middle.png");
            }
        
            .apf_btnDown.redBtn .right {
                background-image : url("images/red_button_right.png");
            }
            
            .apf_btnOver {
            }
        ]]></a:style>
        <a:style condition="apf.isIE7">
            .apf_btn .right {
                margin : 0;
            }
        
            .apf_btn .left {
                margin : 0;
            }
        </a:style>
    
        <a:presentation>
            <a:main 
              caption    = "div[3]/span/text()" 
              background = "."
              minheight  = "25" 
              minwidth   = "30"
              maxheight  = "25">
                <div class="apf_btn">
                    <div class="left"> </div>
                    <div class="right"> </div>
                    <div class="lbl">
                        <span>-</span>
                    </div>
                </div>
            </a:main>
        </a:presentation>
    </a:button>
</a:skin>