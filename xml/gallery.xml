<a:skin xmlns:a="http://ajax.org/2005/aml">
    <a:gallery name="gallery">
        <a:style><![CDATA[
            .apf_gallery {
                font-family      : Tahoma;
                position         : relative;
                width            : 700px;
                overflow         : hidden;
                background-color : white;
            }
            
            .apf_gallery .apfgal_loader {
                width               : 28px;
                height              : 28px;
                position            : absolute;
                top                 : 50%;
                left                : 50%;
                margin              : -14px 0 0 -14px;
                display             : none;
                opacity             : 0;
                background-image    : url(images/loader.gif);
                background-position : 50% 50%;
                background-repeat   : no-repeat;
                background-color    : transparent;
            }

            .apf_gallery .apfgal_image_body {
                position : relative;
                overflow : hidden;
            }
            
            .apf_gallery .apfgal_image_body .apfgal_next {
                position            : absolute;
                top                 : 0;
                right               : 0;
                bottom              : 0;
                width               : 30px;
                background-image    : url(images/ad_next.png);
                background-position : 50% 50%;
                background-repeat   : no-repeat;
                cursor              : pointer;
            }
            
            .apf_gallery .apfgal_image_body .apfgal_previous {
                position            : absolute;
                top                 : 0;
                left                : 0;
                bottom              : 0;
                width               : 30px;
                background-image    : url(images/ad_prev.png);
                background-position : 50% 50%;
                background-repeat   : no-repeat;
                cursor              : pointer;
            }
            
            .apf_gallery .apfgal_image_body .viewport {
                position       : absolute;
                top            : 0;
                right          : 30px;
                left           : 30px;
                bottom         : 0;
                text-align     : center;
                vertical-align : middle;
            }
            
            .apf_gallery .apfgal_image_body .viewport .zoomIcon {
                background-image    : url(images/wide-view-icn.png);
                background-repeat   : no-repeat;
                background-position : 0 0;
                width               : 25px;
                height              : 25px;
                position            : absolute;
                bottom              : 0;
                left                : 0;
                display             : none;
            }
            
            .apf_gallery .apfgal_image_body .viewport .apfgal_image {
                display : none;
                opacity : 0;
            }
            .apf_gallery .apfgal_image_body .viewport .apfgal_image_base {
                display : none;
            }
            
            .apf_gallery .apfgal_bar {
                position : relative;
                width    : 100%;
                margin   : 0;
                padding  : 0;
                overflow : hidden;
            }
            
            .apf_gallery .apfgal_bar .apfgal_descr {
                width      : 100%;
                position   : relative;
                overflow   : hidden;
                padding    : 20px 0;
                color      : #686868;
                text-align : center;
                font-size  : 11px;
            }
            
            .apf_gallery .apfgal_bar .apfgal_thumbs .apfgal_t_body {
                width    : 100%;
                overflow : hidden;
            }
            
            /* ====== GRID MODE ======= */
            
            .gridMode .apfgal_bar .apfgal_thumbs .apfgal_t_body .apfgal_thumbnail {
                height           : 120px;
                width            : 169px;
                float            : left;
                background-color : #efefef;
                border           : 1px solid #cccccc;
                margin           : 2px;
                opacity          : 0.7;
            }
            
            .gridMode .apfgal_bar .apfgal_thumbs .apfgal_t_body .apfgal_thumbnail DIV {
                background-position : 50% 50%;
                background-repeat   : no-repeat;
                width               : 149px;
                height              : 100px;
                margin              : 10px;
                cursor              : pointer;
            }
            
            .gridMode .apfgal_bar .apfgal_thumbs .apfgal_t_body .selected {
                opacity : 1;
            }
            
            .gridMode .apfgal_bar .apfgal_thumbs .apfgal_t_body .selected DIV{
                border : 2px solid #F39719;
                margin : 8px;
            }
            
            /* ====== BAR MODE ======= */
            
            .barMode .apfgal_bar .apfgal_descr {
                padding : 10px 0;
            }
            
            .barMode .apfgal_bar .apfgal_thumbs {
                height           : 49px;
                overflow         : hidden;
                position         : relative;
                background-color : white;
            }
            
            .barMode .apfgal_bar .apfgal_thumbs .apfgal_t_body {
                overflow         : hidden;
                position         : absolute;
                width            : auto;
                right            : 20px;
                left             : 20px;
                height           : 49px;
                top              : 0;
                background-color : white;
            }
            
            .barMode .apfgal_bar .apfgal_thumbs .apfgal_t_body > DIV {
                width            : 9000px;
                height           : 49px;
                position:relative;
            }
            
            .barMode .apfgal_bar .apfgal_thumbs .apfgal_t_body .apfgal_thumbnail {
                width    : 72px;
                height   : 49px;
                overflow : hidden;
                margin   : 0 1px;
                float    : left;
                opacity  : 0.7;
            }
            
            .barMode .apfgal_bar .apfgal_thumbs .apfgal_t_body .apfgal_thumbnail DIV {
                background-position : 50% 50%;
                background-repeat   : no-repeat;
                width               : 68px;
                height              : 45px;
                border              : 2px solid #dbdbdb;
                cursor              : pointer;
            }
            
            .barMode .apfgal_bar .apfgal_thumbs .apfgal_t_body .selected {
                opacity : 1;
            }
            
            .barMode .apfgal_bar .apfgal_thumbs .apfgal_t_body .selected DIV {
                border-color : #f39719;
            }
            
            .barMode .apfgal_bar .apfgal_thumbs .apfgal_t_left_arrow {
                width               : 17px;
                height              : 49px;
                background-color    : #efefef;
                position            : absolute;
                top                 : 0;
                left                : 0;
                border-right        : 3px solid white;
                background-image    : url(images/small-player-left-arr.png);
                background-repeat   : no-repeat;
                background-position : 50% 50%;
                cursor              : pointer;
                opacity             : 0.6;
            }

            .barMode .apfgal_bar .apfgal_thumbs .apfgal_t_right_arrow {
                width               : 17px;
                height              : 49px;
                background-color    : #efefef;
                position            : absolute;
                top                 : 0;
                right               : 0;
                border-left         : 3px solid white;
                background-image    : url(images/small-player-right-arr.png);
                background-repeat   : no-repeat;
                background-position : 50% 50%;
                cursor              : pointer;
                opacity             : 0.6;
            }
            
            .barMode .apfgal_bar .apfgal_thumbs .apfgal_t_left_arrow:hover,
            .barMode .apfgal_bar .apfgal_thumbs .apfgal_t_right_arrow:hover {
                opacity : 1;
            }
            
            .barMode .apfgal_image_body .apfgal_next,
            .barMode .apfgal_image_body .apfgal_previous {
                display:none;
            }
        ]]></a:style>
    
        <a:presentation>
            <a:main 
              container       = "."
              image_container = "div[1]"
              viewport        = "div[1]/div"
              zoom_icon       = "div[1]/div/div"
              next            = "div[1]/div[2]"
              previous        = "div[1]/div[3]"
              loader          = "div[1]/div[4]"
              image           = "div[1]/div[1]/img[1]"
              image_base      = "div[1]/div[1]/img[2]"
              bar             = "div[2]"
              descr           = "div[2]/div[1]"
              thumbs          = "div[2]/div[2]/div[1]/div"
              arrow_prev      = "div[2]/div[2]/div[2]"
              arrow_next      = "div[2]/div[2]/div[3]"
              empty           = "div[3]" >
                <div class="apf_gallery">
                    <div class="apfgal_image_body">
                        <div class="viewport">
                            <img class="apfgal_image" />
                            <img class="apfgal_image_base" />
                            <div class="zoomIcon"> </div>
                        </div>
                        <div class="apfgal_next"> </div>
                        <div class="apfgal_previous"> </div>
                        <div class="apfgal_loader"> </div>
                    </div>
                    <div class="apfgal_bar">
                        <div class="apfgal_descr"> </div>
                        <div class="apfgal_thumbs">
                            <div class="apfgal_t_body">
                                <div> </div>
                            </div>
                            <div class="apfgal_t_left_arrow"> </div>
                            <div class="apfgal_t_right_arrow"> </div>
                        </div>
                    </div>
                    <div class="apfgal_empty"> </div>
                </div>
            </a:main>
            <a:container contents=".">
                <div class="aristo_options"> </div>
            </a:container>
            <a:item class="." select="div" image="div">
                <div class="apfgal_thumbnail">
                    <div> </div>
                </div>
            </a:item>
        </a:presentation>
    </a:gallery>
</a:skin>